# Discord Configuration
DISCORD_BOT_TOKEN=your_real_discord_bot_token
DISCORD_GUILD_ID=your_real_discord_guild_id

# OpenAI Configuration
OPENAI_API_KEY=your_real_openai_api_key
OPENAI_ASSISTANT_ID=your_assistant_id_here

# Google Gemini Configuration
GOOGLE_GEMINI_API_KEY=your_real_google_gemini_api_key

# Weather API Configuration
OPENWEATHER_API_KEY=your_actual_api_key_here

# Application Configuration
PORT=3005
NODE_ENV=development # Change to 'production' in production environment

# Logging Configuration (optional)
LOG_LEVEL=info # Valid values: error, warn, info, debug
LOG_FILE_PATH=logs # Directory where log files will be stored

# Cache Configuration (optional)
CACHE_TTL=3600 # Default cache TTL in seconds
WEATHER_CACHE_TTL=1800 # Weather-specific cache TTL in seconds

# Rate Limiting Configuration (optional)
RATE_LIMIT_WINDOW=60000 # Rate limit window in milliseconds
RATE_LIMIT_MAX_REQUESTS=5 # Maximum requests per window